<view class='search'>
	<view class='searchContent'>
		<input type='text' placeholder='输入你想要的查詢的部門代碼' confirm-type='search' value="{{inputValue}}" bindinput='inputBind'
			bindconfirm='query'></input>
		<image class="" src='/images/search.png' bindtap='query'></image>
	</view>
</view>
<view>
	<block wx:for="{{comfirmList}}" wx:key="index">
		<x-comfirm-list comfirmlist="{{item}}"></x-comfirm-list>
	</block>
</view>